unction change(value) {\r\n      return value\r\n        .replace(/\\D/g, \"\")\r\n        .replace(/([0-9])([0-9]{2})$/, \"$1,$2\")\r\n        .replace(/\\B(?=(\\d{3})+(?!\\d)\\.?)/g, \".\");\r\n    }\r\n    e.target.value = change(e.target.value);\r\n    setProduct({ price: e.target.value });\r\n  };\r\n\r\n  const handleTitle = (e) => {\r\n\r\n    console.log(e.target)\r\n    setProduct({ title: e.target.value });\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <Button variant=\"primary\" onClick={handleShow}>\r\n        Nuevo Producto\r\n      </Button>\r\n\r\n      <Modal size={\"lg\"} show={show} onHide={handleClose}>\r\n        <Modal.Header closeButton>\r\n          <Modal.Title>Nuevo Producto</Modal.Title>\r\n        </Modal.Header>\r\n        <Modal.Body>\r\n          <Form noValidate validated={validated}>\r\n            <Form.Group as={Row}>\r\n              <Form.Label column sm={3}>\r\n                Categor&iacute;a\r\n              </Form.Label>\r\n              <Col sm={9}>\r\n                <Form.Control\r\n                  onChange={(e) => {\r\n                    setProduct({ category: e.target.name.value });\r\n                  }}\r\n                  name=\"category\"\r\n                  id=\"category\"\r\n                  required\r\n                  as=\"select\"\r\n                  defaultValue=\"Seleccionar...\"\r\n                >\r\n                  <option value=\"\">Seleccionar...</option>\r\n                  {categories.map((i) => (\r\n                    <option key={i._id} value={i._id}>\r\n                      {i.title}\r\n                    </option>\r\n                  ))}\r\n                </Form.Control>\r\n                <Form.Control.Feedback>Correcto!</Form.Control.Feedback>\r\n                <Form.Control.Feedback type=\"invalid\">\r\n                  Seleccione una categor&iacute;a\r\n                </Form.Control.Feedback>\r\n              </Col>\r\n            </Form.Group>\r\n\r\n            <Form.Group as={Row} style={{ marginTop: \"40px\" }}>\r\n              <Form.Label column sm={3}>\r\n                Nombre Producto\r\n              </Form.Label>\r\n              <Col sm={9}>\r\n                <FormControl\r\n                  onChange={handleTitle}\r\n                  name=\"title\"\r\n                  id=\"title\"\r\n                  required\r\n                  type=\"text\"\r\n                  placeholder=\"Nombre Producto\"\r\n                />\r\n                <Form.Control.Feedback>Correcto!</Form.Control.Feedback>\r\n                <Form.Control.Feedback type=\"invalid\">\r\n                  Introduzca el nombre del producto.\r\n                </Form.Control.Feedback>\r\n              </Col>\r\n            </Form.Group>\r\n\r\n            <Form.Group as={Row}>\r\n              <Form.Label column sm={3}>\r\n                Precio\r\n              </Form.Label>\r\n              <Col sm={9}>\r\n                <InputGroup className=\"mb-2\">\r\n                  <InputGroup.Prepend>\r\n                    <InputGroup.Text>$</InputGroup.Text>\r\n                  </InputGroup.Prepend>\r\n                  <FormControl\r\n                    onKeyUp={handleKeyUp}\r\n                    name=\"price\"\r\n                    id=\"price\"\r\n                    type=\"text\"\r\n                    required\r\n                    placeholder=\"Precio\"\r\n                  />\r\n                  <Form.Control.Feedback type=\"invalid\">\r\n                    Introduzca el precio en D&oacute;lares.\r\n                  </Form.Control.Feedback>\r\n                  <Form.Control.Feedback>Correcto!</Form.Control.Feedback>\r\n                </InputGroup>\r\n              </Col>\r\n            </Form.Group>\r\n\r\n            <Form.Group as={Row}>\r\n              <Form.Label column sm={3}>\r\n                C&oacute;digo\r\n              </Form.Label>\r\n              <Col sm={9}>\r\n                <Form.Control\r\n                  required\r\n                  id=\"codebar\"\r\n                  name=\"codebar\"\r\n                  type=\"text\"\r\n                  placeholder=\"C&oacute;digo de barra\"\r\n                />\r\n                <Form.Control.Feedback>Correcto!</Form.Control.Feedback>\r\n                <Form.Control.Feedback type=\"invalid\">\r\n                  Introduzca el c&oacute;digo de barras. escaneando el producto\r\n                  con el lector de barras proporcionado.\r\n                </Form.Control.Feedback>\r\n              </Col>\r\n            </Form.Group>\r\n          </Form>\r\n        </Modal.Body>\r\n        <Modal.Footer>\r\n          <Button variant=\"secondary\" onClick={handleClose}>\r\n            Cerrar\r\n          </Button>\r\n          <Button variant=\"primary\" onClick={handleSubmit}>\r\n            Guardar producto\r\n          </Button>\r\n        </Modal.Footer>\r\n      </Modal>\r\n    </>\r\n  );\r\n}\r\n"]},"metadata":{},"sourceType":"module"}