> setShow(false);\r\n  const handleShow = () => setShow(true);\r\n\r\n  return (\r\n    <>\r\n      <Button variant=\"info\" onClick={handleShow}>\r\n        Nuevo Usuario\r\n      </Button>\r\n\r\n      <Modal size={\"lg\"} show={show} onHide={handleClose}>\r\n        <Modal.Header closeButton>\r\n          <Modal.Title>Nuevo usuario</Modal.Title>\r\n        </Modal.Header>\r\n        <Modal.Body>\r\n          <Form validated={validated}>\r\n            <Form.Group as={Row}>\r\n              <Form.Label column sm={3}>\r\n                Rol\r\n              </Form.Label>\r\n              <Col sm={9}>\r\n                <Form.Control\r\n                  onChange={(e) => {\r\n                    setUser({ ...user, id_role: e.target.value });\r\n                  }}\r\n                  name=\"id_role\"\r\n                  id=\"id_role\"\r\n                  required\r\n                  as=\"select\"\r\n                  defaultValue=\"Seleccionar...\"\r\n                >\r\n                  <option value=\"\">Seleccionar...</option>\r\n                  <option value=\"6079ab4c3c6b1e091c985626\">Admin</option>\r\n                  <option value=\"6079ab4c3c6b1e091c985625\">User</option>\r\n                </Form.Control>\r\n                <Form.Control.Feedback>Correcto!</Form.Control.Feedback>\r\n                <Form.Control.Feedback type=\"invalid\">\r\n                  Seleccione un rol\r\n                </Form.Control.Feedback>\r\n              </Col>\r\n            </Form.Group>\r\n\r\n            <Form.Group as={Row} style={{ marginTop: \"30px\" }}>\r\n              <Form.Label column sm={3}>\r\n                Nombre\r\n              </Form.Label>\r\n              <Col sm={9}>\r\n                <FormControl\r\n                  onChange={(e) => {\r\n                    setUser({ ...user, username: e.target.value });\r\n                  }}\r\n                  name=\"username\"\r\n                  id=\"username\"\r\n                  required\r\n                  value={user.username}\r\n                  type=\"text\"\r\n                  placeholder=\"Nombre\"\r\n                />\r\n                <Form.Control.Feedback>Correcto!</Form.Control.Feedback>\r\n                <Form.Control.Feedback type=\"invalid\">\r\n                  Introduzca el nombre del usuario\r\n                </Form.Control.Feedback>\r\n              </Col>\r\n            </Form.Group>\r\n\r\n            <Form.Group as={Row} style={{ marginTop: \"30px\" }}>\r\n              <Form.Label column sm={3}>\r\n                Email\r\n              </Form.Label>\r\n              <Col sm={9}>\r\n                <FormControl\r\n                  onChange={(e) => {\r\n                    setUser({ ...user, email: e.target.value });\r\n                  }}\r\n                  name=\"email\"\r\n                  id=\"email\"\r\n                  required\r\n                  value={user.email}\r\n                  type=\"text\"\r\n                  placeholder=\"Nombre\"\r\n                />\r\n                <Form.Control.Feedback>Correcto!</Form.Control.Feedback>\r\n                <Form.Control.Feedback type=\"invalid\">\r\n                  Introduzca el email\r\n                </Form.Control.Feedback>\r\n              </Col>\r\n            </Form.Group>\r\n          </Form>\r\n        </Modal.Body>\r\n        <Modal.Footer>\r\n          <Button variant=\"secondary\" onClick={handleClose}>\r\n            Cerrar\r\n          </Button>\r\n          <Button variant=\"primary\" onClick={handleSubmit}>\r\n            Guardar usuario\r\n          </Button>\r\n        </Modal.Footer>\r\n      </Modal>\r\n    </>\r\n  );\r\n}\r\n"]},"metadata":{},"sourceType":"module"}